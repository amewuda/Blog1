<%- include ("partials/header"); %>
<h3>This is for the home page</h3>



<%= blogs%>

<%blogs.forEach(function(blog){%>
    <div>
        <h2><%=blog.title%></h2>
        <img src="<%=blog.image%>">
        <span><%=blog.created%></span>
        <p><%=blog.body%></p>
        <a href="/blogs/<%=blog._id%>">Read more</a>
    </div>
    <em><hr></em>
<%})%>

<%- include ("partials/footer"); %>